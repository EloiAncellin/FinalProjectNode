{
    "name": "Node-it",
    "version": "0.1.0",
    "dependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.17",
        "body-parser": "^1.19.0",
        "custom-env": "^1.0.2",
        "ejs": "^2.7.4",
        "express": "^4.17.1",
        "mongodb": "^3.3.5",
        "password-hash": "^1.2.2",
        "typescript": "^3.7.3"
    },
    "scripts": {
        "build": "node_modules/typescript/bin/tsc",
        "start": "npm run build && node src/dist/app.js",
        "dev": "nodemon -e ts --exec \"npm run start || exit 1\"",
        "test": "npm run build && mocha src/dist/test",
        "coverage": "nyc npm run test",
        "test-travis": "nyc --reporter=lcov npm run test",
        "coveralls": "cat coverage/lcov.info | coveralls"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "coveralls": "^3.0.9",
        "mocha": "^6.2.2",
        "mocha-lcov-reporter": "^1.3.0",
        "nodemon": "^2.0.1",
        "nyc": "^14.1.1"
    }
}
