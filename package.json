{
    "name": "Node-it",
    "version": "0.1.0",
    "dependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.17",
        "bcrypt": "^3.0.7",
        "body-parser": "^1.19.0",
        "colors": "^1.4.0",
        "dotenv": "^8.2.0",
        "ejs": "^2.7.4",
        "express": "^4.17.1",
        "fs": "0.0.1-security",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^3.3.5",
        "mongoose": "^5.8.1",
        "node-rsa": "^1.0.7",
        "typescript": "^3.7.3"
    },
    "scripts": {
        "build": "node_modules/typescript/bin/tsc && npm run generate-keys",
        "generate-keys": "node src/dist/keys/generate.js",
        "populate": "npm run build && node src/dist/utils/populate",
        "start": "npm run build && node src/dist/app.js",
        "dev": "nodemon -e ts --exec \"npm run start || exit 1\"",
        "test": "npm run build && mocha src/dist/test",
        "coverage": "nyc npm run test",
        "test-travis": "nyc --reporter=lcov npm run test",
        "coveralls": "cat coverage/lcov.info | coveralls"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "coveralls": "^3.0.9",
        "deep-equal-in-any-order": "^1.0.21",
        "mocha": "^6.2.2",
        "mocha-lcov-reporter": "^1.3.0",
        "nodemon": "^2.0.1",
        "nyc": "^14.1.1",
        "supertest": "^4.0.2",
        "underscore": "^1.9.1"
    }
}
